mysql> SELECT 
    ->     first_name, IFNULL(AVG(grade), 0) AS average
    -> FROM
    ->     students
    ->         LEFT JOIN
    ->     papers ON students.id = papers.student_id
    -> GROUP BY first_NAME;
+------------+---------+
| first_name | average |
+------------+---------+
| Caleb      |    67.5 |
| Samantha   |      96 |
| Raj        |       0 |
| Carlos     |      89 |
| Lisa       |       0 |
+------------+---------+
5 rows in set (0.00 sec)

mysql> SELECT      first_name, IFNULL(AVG(grade), 0) AS average FROM     students         LEFT JOIN     papers ON students.id = papers.student_id GROUP BY first_NAME order by average desc;
+------------+---------+
| first_name | average |
+------------+---------+
| Samantha   |      96 |
| Carlos     |      89 |
| Caleb      |    67.5 |
| Raj        |       0 |
| Lisa       |       0 |
+------------+---------+
5 rows in set (0.00 sec)

mysql> SELECT      first_name, IFNULL(AVG(grade), 0) AS average FROM     students         LEFT JOIN     papers ON students.id = papers.student_id GROUP BY first_NAME order by average;
+------------+---------+
| first_name | average |
+------------+---------+
| Raj        |       0 |
| Lisa       |       0 |
| Caleb      |    67.5 |
| Carlos     |      89 |
| Samantha   |      96 |
+------------+---------+
5 rows in set (0.00 sec)

mysql> SELECT 
    ->     first_name, IFNULL(AVG(grade), 0) AS average
    -> FROM
    ->     students
    ->         LEFT JOIN
    ->     papers ON students.id = papers.student_id
    -> GROUP BY first_NAME;
+------------+---------+
| first_name | average |
+------------+---------+
| Caleb      |    67.5 |
| Samantha   |      96 |
| Raj        |       0 |
| Carlos     |      89 |
| Lisa       |       0 |
+------------+---------+
5 rows in set (0.00 sec)

mysql> SELECT 
    ->     first_name,
    ->     IFNULL(AVG(GRADE), 0) AS average,
    ->     CASE
    ->         WHEN AVG(grade) >= 75 THEN 'Passing'
    ->         ELSE 'Failing'
    ->     END AS Passing_Status
    -> FROM
    ->     students
    ->         LEFT JOIN
    ->     papers ON students.id = papers.student_id
    -> GROUP BY first_Name
    -> ORDER BY average DESC;
+------------+---------+----------------+
| first_name | average | Passing_Status |
+------------+---------+----------------+
| Samantha   |      96 | Passing        |
| Carlos     |      89 | Passing        |
| Caleb      |    67.5 | Failing        |
| Raj        |       0 | Failing        |
| Lisa       |       0 | Failing        |
+------------+---------+----------------+
5 rows in set (0.00 sec)

mysql> 
