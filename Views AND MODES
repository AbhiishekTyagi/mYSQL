mysql> SELECT 
    ->     title,released_year,genre,rating,first_name,last_Name 
    ->     
    -> FROM
    ->     reviews
    ->         INNER JOIN
    ->     series ON reviews.series_id = series.id
    ->         INNER JOIN
    ->     reviewers on
    ->      reviewers.id=reviews.reviewer_id;
