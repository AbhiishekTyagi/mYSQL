mysql> SELECT 
    ->     title, released_year, Rating
    -> FROM
    ->     full_review
    -> WHERE
    ->     rating < (SELECT 
    ->             MAX(rating) AS HighestRating
    ->         FROM
    ->             full_review)
    -> LIMIT 1;
