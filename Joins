mysql> SELECT 
    ->     First_Name, Last_Name, max(amount) as Highest
    -> FROM
    ->     customers
    ->         INNER JOIN
    ->     orders ON orders.customer_id = customers.cust_id
    -> GROUP BY first_Name , Last_Name
    -> order by Highest desc;
