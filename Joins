    ->     First_Name, Last_Name, count(*) as Num
    -> FROM
    ->     customers
    ->         INNER JOIN
    ->     orders ON orders.customer_id = customers.cust_id
    -> GROUP BY first_Name , Last_Name
    -> order by num desc;
