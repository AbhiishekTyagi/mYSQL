mysql> select author_fname,author_lname,count(*),
    -> Case
    -> when count(*)=1 then '1 Book'
    -> else  concat(count(*),' books')
    -> End as Count
    -> from books
    -> where author_lname is not null
    -> group by author_fname,author_lname;    
