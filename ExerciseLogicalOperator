mysql> -- Create table foreign 
mysql> create table foreign
    -> (
    -> FullName varchar(12)not null,
    -> Id tinyint primary key,
    -> Fathername varchar(15) not null,
    -> cust_id tinyint foreign key(cust_id) references prim(cust_id)
    -> );
