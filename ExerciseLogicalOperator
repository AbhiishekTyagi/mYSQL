    -> Case
    -> when title like '%Stories%' then 'Short Stories'
    -> when title like 'just Kids' then 'Memoir'
    -> when title like 'A Heartbreaking Work of Staggering Genius' then 'Memoir'
    -> else 'Novel'
    -> End as Type
    -> from books;
